  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "docs": "typedoc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./jest-e2e.config.ts",
    "seed": "npm run build && node -r tsconfig-paths/register dist/src/database/seed.js",
    "seed:dev": "ts-node --transpile-only --skip-project -r tsconfig-paths/register src/database/seed.ts"
  },
